<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
  </PropertyGroup>

  <ItemGroup Condition="'$(AppVariant)'=='' Or '$(AppVariant)'=='single'">
    <Compile Include="Program.cs" />
  </ItemGroup>
  <ItemGroup Condition="'$(AppVariant)'=='batch'">
    <Compile Include="Program-batch.cs" />
  </ItemGroup>
  <ItemGroup Condition="'$(AppVariant)'=='batch-parallel'">
    <Compile Include="Program-batch-parallel.cs" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="cache\" />
  </ItemGroup>

</Project>
